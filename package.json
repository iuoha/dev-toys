{
  "name": "esbuild-dev-environment-sample.git",
  "version": "1.0.0",
  "main": "public/index.js",
  "scripts": {
    "build": "node --require esbuild-register build.ts",
    "compile": "tsc",
    "test": "npm run compile && jest",
    "test:watch": "npm rum test -- --watch",
    "dev": "concurrently --prefix \"{time}\" -t \"HH:mm:ss\" -c \"bgCyan.bold,bgGreen.bold,bgGrey.bold\" -n \"esbuild,BrowserSync,Express\" \"npm run build:watch\" \"npm run browser:reload\" \"npm run serve:local\"",
    "build:watch": "WATCH=true npm run build",
    "browser:reload": "wait-on public/index.js && browser-sync start --notify=false --proxy 'http://localhost:3030' --files 'public/*' --port 3000",
    "serve:local": "node --require esbuild-register local-hosting.ts",
    "metafile": "NODE_ENV=production META_FILE=true npm run build"
  },
  "author": "hedrall",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "15.14.9",
    "@types/react": "17.0.30",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.3.1",
    "browser-sync": "2.27.5",
    "esbuild": "0.13.4",
    "esbuild-register": "3.0.0",
    "eslint": "7.32.0",
    "eslint-config-react-app": "6.0.0",
    "express": "4.17.1",
    "jest": "27.2.5",
    "jest-dom": "4.0.0",
    "react": "17.0.2",
    "react-router-dom": "5.3.0",
    "react-test-renderer": "17.0.2",
    "ts-jest": "27.0.5",
    "typescript": "4.4.3",
    "wait-on": "6.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.4.4",
    "@mui/material": "^5.4.4",
    "antd": "4.16.13",
    "axios": "^0.26.0",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "concurrently": "6.3.0",
    "dayjs": "1.10.7",
    "leader-line-new": "^1.1.9",
    "react-bootstrap": "^2.2.0",
    "react-dom": "17.0.2",
    "react-query": "3.25.1",
    "recoil": "^0.6.1"
  }
}
